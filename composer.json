{
  "name": "xlerr/oplogger",
  "description": "Yii2 operational logger",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "xlerr",
      "email": "xlerr@qq.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0",
    "danielstjules/stringy": "^3.1"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "oplogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "oplogger\\test\\": "test/"
    }
  },
  "config": {
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
  }
}
